<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="vn.hiep.demobilling">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:name=".BillingApplication"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:theme="@style/Theme.DemoBilling">
        <activity android:name=".ui.SubscribeActivity" />
        <activity android:name=".ui.OldMainActivity" />
        <activity
            android:name=".ui.MainBillingActivity"
            android:screenOrientation="fullSensor">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>